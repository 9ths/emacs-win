#! /usr/bin/make -f

# [[https://www.gnu.org/prep/standards/html_node/Makefile-Basics.html#Makefile-Basics][Every Makefile should contain this line:]]
SHELL = /bin/sh

.PHONY: clean
clean:
	rm --force  *.el[cn] {,site-}lisp/*.el[cn]
	rm --force  README.html*
	rm --force  docs/*.{html,pdf,tex}*

